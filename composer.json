{
    "name": "zf2-boiler-app/app-messenger",
    "license": "MIT",
   	"type": "library",
    "description": "ZF2 BoilerApp Messenger module",
    "keywords": ["Zend Framework 2","ZF2 Boiler-App","Messenger","mail"],
    "authors": [
        {
            "name": "Neilime",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/zf2-boiler-app/app-messenger/issues"
    },
    "minimum-stability" : "dev",
    "repositories":[        
        {
	        "type": "vcs",
	        "url": "http://github.com/Nodge/lessphp"
	    },
        {
            "type": "package",
            "package": {
                "version": "dev-master",
                "name": "fabiomcosta/mootools-meio-mask",
                "source": {"url": "https://github.com/fabiomcosta/mootools-meio-mask.git","type": "git","reference": "master"}
            }
        },
        {
            "type": "package",
            "package": {
                "version": "dev-master",
                "name": "arian/iFrameFormRequest",
                "source": {"url": "https://github.com/arian/iFrameFormRequest.git","type": "git","reference": "master"}
            }
        },
        {
            "type": "package",
            "package": {
                "version": "dev-master",
                "name": "nak5ive/Form.PasswordStrength",
                "source": {"url": "https://github.com/nak5ive/Form.PasswordStrength.git","type": "git","reference": "master"}
            }
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "ext-fileinfo": "*", 
        "zendframework/zendframework": "2.3.*"          
    },
    "require-dev":{
    	"zf2-boiler-app/app-test": "1.0.*",
    	"neilime/zf2-tree-layout-stack": "dev-master",
        "neilime/zf2-assets-bundle": "1.0.*",
        "inlinestyle/inlinestyle": "dev-master",
        "tijsverkoyen/css-to-inline-styles": "dev-master"
    },
    "suggest": {
        "neilime/zf2-tree-layout-stack": "Tree layout for email rendering",
        "neilime/zf2-assets-bundle": "Manage assets for email rendering",
        "inlinestyle/inlinestyle": "Inlinestyle processor for email rendering",
        "tijsverkoyen/css-to-inline-styles": "CssToInlineStyles processor for email rendering"
    },
    "autoload": {
        "psr-0": {
            "BoilerAppMessenger": "src/"
        },
        "classmap" : ["Module.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
